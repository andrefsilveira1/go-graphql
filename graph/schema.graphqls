# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Todo {
  id: ID!
  text: String!
  done: Boolean!
  user: User!
}

type User {
  id: ID!
  name: String!
}

type Query {
  todos: [Todo!]!
}

input NewTodo {
  text: String!
  userId: String!
}

type Character {
  id: ID!
  name: String!
}

input CharacterInput {
  name: String!
  id: String
}

type Mutation {
  upsertCharacter(input: CharacterInput!): Character!
}